<?xml version="1.0" encoding="utf-8"?>
<!--
    Nav graph for the like overview, displaying all users
    that have liked any of the current users vlogs.
-->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_like_overview"
    app:startDestination="@id/like_overview_dest">

    <fragment
        android:id="@+id/like_overview_dest"
        android:name="com.laixer.swabbr.presentation.likeoverview.LikeOverviewFragment"
        android:label="Vlog"
        tools:layout="@layout/fragment_like_overview">
        <argument
            android:name="vlogId"
            app:argType="string" />
        <deepLink
            android:id="@+id/deepLink"
            android:autoVerify="true"
            app:uri="https://swabbr.com/like-overview" />
    </fragment>

</navigation>
