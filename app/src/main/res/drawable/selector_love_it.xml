<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- When selected, use filled icon. -->
    <item android:drawable="@drawable/ic_love_it"
        android:state_checked="true" />
    <!-- When not selected, outline. -->
    <item android:drawable="@drawable/ic_love_it_outline"
        android:state_checked="false"/>
</selector>
